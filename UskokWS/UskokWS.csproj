<Project Sdk="Microsoft.NET.Sdk.Web">

    <PropertyGroup>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <OutputType>Library</OutputType>
        <Title>Websocket helper</Title>
        <Description>A little library that helps with using webosckets in asp.net</Description>
        <RepositoryUrl>https://github.com/vukuskokovic/UskokWS</RepositoryUrl>
        <IsPackable>true</IsPackable>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <TargetFrameworks>net7.0;net6.0</TargetFrameworks>
        <Version>1.1.0</Version>
        <PackageId>UskokWebSocket</PackageId>
        <Authors>Vuk Uskokovic</Authors>
    </PropertyGroup>

</Project>
